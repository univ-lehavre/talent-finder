{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["src/lib"],
	"entryPointStrategy": "expand",
	"out": "api-docs",
	"exclude": ["**/*.test.ts", "**/*.spec.ts", "**/index.ts"],
	"excludePrivate": true,
	"excludeProtected": true,
	"includeVersion": true,
	"readme": "none",
	"navigationLinks": {
		"GitHub": "https://github.com/univ-lehavre/talent-finder"
	},
	"plugin": [],
	"hideGenerator": true,
	"searchInComments": true,
	"categorizeByGroup": true,
	"sort": ["source-order"],
	"highlightLanguages": ["typescript", "javascript", "svelte", "html", "css", "json", "bash"]
}
